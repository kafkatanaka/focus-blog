---
interface Props {
  slot?: string;
  format?: string;
}

const { slot = '4412571347', format = 'auto' } = Astro.props;
---

<ins
  class="adsbygoogle block"
  style="display:block"
  data-ad-client="ca-pub-2329837198490466"
  data-ad-slot={slot}
  data-ad-format={format}
  data-full-width-responsive="true"
></ins>
<script is:inline>
  (adsbygoogle = window.adsbygoogle || []).push({});
</script>
